#include <iostream>
using namespace std;

template <class T>
void InsertionSort(T data[],int n){
	int p;
	for(p=1;p<n;p++){
		for(int i=p-1;i>=0;i--){
			if(data[i]>data[p]){
				data[i+1]=data[i];
			}
		}
	}
}

int main() {
	return 0;
}
